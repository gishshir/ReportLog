<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='reportlog'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
  <inherits name="com.google.gwt.logging.Logging"/>
  <!-- Drag and drop -->
   <inherits name='gwtquery.plugins.droppable.Droppable'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='fr.tsadeo.app.gwt.reportlog.client.Reportlog'/>
   
  <stylesheet src="css/GwtWidget.css"/>
  <stylesheet src="css/ReportLogView.css" />
  <stylesheet src="advanced/themes/default/theme.css"/>
  
   <set-property name="user.agent"  value="gecko1_8" />
 <!--  <set-property name="user.agent" value="safari, gecko1_8, ie8" />-->

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  
     <!-- logging -->
    <set-property name="gwt.logging.logLevel" value="CONFIG"/>           # To change the default logLevel
    <set-property name="gwt.logging.enabled" value="FALSE"/>            # To disable logging
    <set-property name="gwt.logging.consoleHandler" value="ENABLED" />


</module>
